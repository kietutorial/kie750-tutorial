    <sequenceFlow id="_2-1d4d37c9-32db-4c43-97d6-b26a7fc69b00" sourceRef="_2" targetRef="1d4d37c9-32db-4c43-97d6-b26a7fc69b00" />
    <sequenceFlow id="4254e06d-d837-4ad0-9476-9b4be34e0b0a-1d4d37c9-32db-4c43-97d6-b26a7fc69b00" sourceRef="4254e06d-d837-4ad0-9476-9b4be34e0b0a" targetRef="1d4d37c9-32db-4c43-97d6-b26a7fc69b00" name="Is Not Hello" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >if (message.getStatus() != Message.HELLO) {
	return true;
}

return false;	</conditionExpression>
    </sequenceFlow>

