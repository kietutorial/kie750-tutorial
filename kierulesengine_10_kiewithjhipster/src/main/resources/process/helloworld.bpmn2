        </tns:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
      <bpmn2:script>ProcessUtil.showTask(kcontext);
ProcessUtil.showFact(message);

if ( message.getMessage().indexOf(&quot;Hello&quot;) >=0 ) {
	message.setMessage(&quot;Goodbye Cruel World&quot;);
	message.setStatus( StatusType.GOODBYE );
	ProcessUtil.showMessage(&quot;Changed message status cause message started with 'Hello'&quot;); 

	ProcessUtil.showFact(message);
}
</bpmn2:script>
    </bpmn2:scriptTask>
